{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "kvname": {
            "type": "string",
            "metadata": {
                "description": "description"
            },
            "defaultValue": "vmkv3003"
        },
        "secretType": {
            "type": "string",
            "metadata": {
                "description": "description"
            },
            "defaultValue": "Microsoft.KeyVault/vaults/secrets"
        },
        "adminName": {
            "type": "string",
            "metadata": {
                "description": "description"
            },
            "defaultValue": "winadmin"
        },
        "kvType": {
            "type": "string",
            "metadata": {
                "description": "description"
            },
            "defaultValue": "Microsoft.KeyVault/vaults"
        },
        "api": {
            "type": "string",
            "metadata": {
                "description": "description"
            },
            "defaultValue": "2023-07-01"
        }
    },
    "functions": [],
    "variables": {},
    "resources": [],
    "outputs": {
        "secret": {
            "type": "Object",
            "value": "[reference(resourceId(parameters('secretType'),parameters('kvname'),parameters('adminName')),parameters('api'), 'Full')]"
        },
        "kv": {
            "type": "Object",
            "value": "[reference(resourceId(parameters('kvType'),parameters('kvname')),parameters('api'), 'Full')]"
        }
    }
}